    "documentation": "^4.0.0-rc.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "jquery": "^2.0.0",
    "karma": "1.6.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-webpack": "2.0.3",
    "mocha": "^3.4.2",
